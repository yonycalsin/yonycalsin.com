    "clsx": "^1.1.1",
    "contentlayer": "0.0.31",
    "dayjs": "^1.10.7",
    "debug": "^4.3.2",
    "flexbox-fast": "^1.0.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "just-is-empty": "^3.1.1",
    "just-map-values": "^3.0.2",
    "just-pick": "^4.0.1",
    "just-random": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "^12.0.7",
    "next-connect": "^0.11.0",
    "next-contentlayer": "0.0.31",
    "next-joi": "^2.2.1",
    "nextjs-cors": "^2.1.0",
    "nookies": "^2.5.2",
    "nprogress": "^0.2.0",
    "prisma": "^3.5.0",
