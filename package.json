{
  "name": "@yonycalsin/yonycalsin.com",
  "version": "2.0.1",
  "private": true,
  "description": "Personal website running on Nextjs, Typescript, React, and Node.js.",
  "keywords": [],
  "author": "Yony Calsin",
  "main": "n/a",
  "scripts": {
    "boot": "run-s clean build start",
    "build": "pnpm exec next build",
    "check:format": "pnpm run prettier --list-different",
    "clean": "pnpm exec rimraf -rf .next .contentlayer .vercel coverage storybook-build",
    "contentlayer:build": "pnpm exec contentlayer build",
    "dev": " pnpm exec next dev",
    "fetch:submodules": "pnpm run script:run scripts/fetch-private-submodules/index.ts",
    "format": "pnpm run prettier -- --write",
    "lint": "pnpm exec eslint --max-warnings=0 --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "pnpm run lint -- --fix",
    "lint:ts": "pnpm exec tsc --noEmit",
    "vercel-build": "pnpm exec run-s fetch:submodules build",
    "prettier": "pnpm exec prettier \"**/*.{js,json,ts,tsx,scss,css,md}\"",
    "script:run": "pnpm exec ts-node --project scripts/tsconfig.json",
    "start": "pnpm exec next start",
    "storybook": "pnpm exec run-s storybook:clean storybook:dev",
    "storybook:build": "pnpm run contentlayer:build && pnpm exec build-storybook -s public -o storybook-build",
    "storybook:clean": "pnpm exec rimraf -r node_modules/.cache storybook-build",
    "storybook:dev": "pnpm exec start-storybook -p 6006 -s ./public",
    "test:static": "pnpm exec run-p check:format lint lint:ts",
    "test:unit": "pnpm exec jest --watch --watchAll=true --forceExit --detectOpenHandles --env=jsdom --colors",
    "test:unit:ci": "pnpm exec jest --ci --watchAll=false --coverage --forceExit --detectOpenHandles --env=jsdom --colors",
    "test:e2e": "pnpm exec is-ci-cli \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "pnpm run build && pnpm exec start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "pnpm exec start-server-and-test dev http://localhost:3000 cy:open",
    "cy:run": "pnpm exec cypress run",
    "cy:open": "pnpm exec cypress open",
    "prepare": "pnpm exec husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "@chaitin/querystring": "1.1.0",
    "@chakra-ui/react": "2.2.6",
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@tanstack/react-query": "4.1.3",
    "contentlayer": "0.2.7",
    "dayjs": "1.11.5",
    "debug": "4.3.4",
    "framer-motion": "7.1.0",
    "just-is-empty": "3.2.1",
    "just-map-values": "3.1.1",
    "just-random": "3.1.1",
    "lodash": "4.17.21",
    "ms": "2.1.3",
    "next": "12.2.5",
    "next-base64": "1.1.0",
    "next-connect": "0.13.0",
    "next-contentlayer": "0.2.7",
    "nextjs-cors": "2.1.1",
    "nprogress": "0.2.0",
    "prism-theme-night-owl": "1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.1",
    "react-icons": "4.4.0",
    "react-medium-image-zoom": "5.0.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-code-titles": "1.1.0",
    "rehype-prism-plus": "1.4.2",
    "rehype-slug": "5.0.1",
    "remark-gfm": "3.0.1",
    "toggled": "2.1.0",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@dotenv-cloak/cli": "2.0.0",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/addon-storysource": "6.5.10",
    "@storybook/builder-webpack5": "6.5.10",
    "@storybook/manager-webpack5": "6.5.10",
    "@storybook/react": "6.5.10",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/debug": "4.1.7",
    "@types/jest": "28.1.6",
    "@types/lodash": "4.14.182",
    "@types/ms": "0.7.31",
    "@types/node": "18.7.3",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "@yonycalsin/eslint-plugin-import-sort": "1.0.7",
    "@yonycalsin/prettier-config-react-typescript": "1.0.1",
    "babel-loader": "8.2.5",
    "cypress": "10.4.0",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-storybook": "0.6.4",
    "eslint-plugin-testing-library": "5.6.0",
    "husky": "8.0.1",
    "is-ci-cli": "2.2.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "2.2.18",
    "react-test-renderer": "18.2.0",
    "rimraf": "3.0.2",
    "start-server-and-test": "1.14.0",
    "storybook-addon-themes": "6.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.7.4",
    "wait-on": "6.0.1",
