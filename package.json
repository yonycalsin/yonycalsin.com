{
  "name": "@yonycalsin/yonycalsin.com",
  "version": "2.0.1",
  "private": true,
  "description": "Personal website running on Nextjs, Typescript, React, and Node.js.",
  "keywords": [],
  "author": "Yony Calsin",
  "main": "n/a",
  "scripts": {
    "boot": "run-s clean build start",
    "build": "pnpm exec next build",
    "check:format": "pnpm run prettier --list-different",
    "clean": "pnpm exec rimraf -rf .next .vercel coverage storybook-build",
    "dev": " pnpm exec next dev",
    "format": "pnpm run prettier -- --write",
    "lint": "pnpm exec eslint --max-warnings=0 --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "pnpm run lint -- --fix",
    "lint:ts": "pnpm exec tsc --noEmit",
    "prettier": "pnpm exec prettier \"**/*.{js,json,ts,tsx,scss,css,md}\"",
    "start": "pnpm exec next start",
    "storybook": "pnpm exec run-s storybook:clean storybook:dev",
    "storybook:build": "pnpm exec build-storybook -s public -o storybook-build",
    "storybook:clean": "pnpm exec rimraf -r node_modules/.cache storybook-build",
    "storybook:dev": "pnpm exec start-storybook -p 6006 -s ./public",
    "test:static": "pnpm exec run-p check:format lint lint:ts",
    "test:unit": "cross-env TZ=America/Los_Angeles jest --watch --watchAll=true --forceExit --detectOpenHandles --env=jsdom --colors",
    "test:unit:watch": "cross-env TZ=America/Los_Angeles jest --watch --watchAll=false --detectOpenHandles --env=jsdom --colors",
    "test:unit:ci": "cross-env TZ=America/Los_Angeles jest --ci --watchAll=false --coverage --forceExit --detectOpenHandles --env=jsdom --colors",
    "test:e2e": "pnpm exec is-ci-cli \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "pnpm run build && pnpm exec start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "pnpm exec start-server-and-test dev http://localhost:3000 cy:open",
    "cy:run": "pnpm exec cypress run",
    "cy:open": "pnpm exec cypress open",
    "prepare": "pnpm exec husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "2.3.2",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@tanstack/react-query": "4.3.4",
    "dayjs": "1.11.5",
    "debug": "4.3.4",
    "framer-motion": "7.3.5",
    "just-random": "3.1.1",
    "lodash": "4.17.21",
    "ms": "2.1.3",
    "msw": "0.44.2",
    "next": "12.3.0",
    "next-base64": "1.1.0",
    "next-connect": "0.13.0",
    "nextjs-cors": "2.1.1",
    "nprogress": "0.2.0",
    "prism-theme-night-owl": "1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.35.0",
    "react-icons": "4.4.0",
    "react-medium-image-zoom": "5.0.2",
    "toggled": "2.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@dotenv-cloak/cli": "2.0.0",
    "@emotion/jest": "11.10.0",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/addon-storysource": "6.5.12",
    "@storybook/builder-webpack5": "6.5.12",
    "@storybook/manager-webpack5": "6.5.12",
    "@storybook/react": "6.5.12",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/debug": "4.1.7",
    "@types/gtag.js": "0.0.10",
    "@types/jest": "28.1.8",
    "@types/lodash": "4.14.185",
    "@types/ms": "0.7.31",
