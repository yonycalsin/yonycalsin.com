{
  "name": "@yonycalsin/yonycalsin.com",
  "version": "0.0.0",
  "private": true,
  "description": "Personal website running on Nextjs, Typescript, React, and Node.js.",
  "author": "Yony Calsin",
  "main": "n/a",
  "scripts": {
    "lib:ts": "tsc",
    "lib:es": "eslint --max-warnings=0 --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lib:pretty": "prettier --ignore-path .gitignore \"**/*.{js,json,ts,tsx,scss,css,md}\"",
    "lib:jest": "cross-env TZ=\"America/Los_Angeles\" jest --env=jsdom --colors",
    "start": "next start",
    "build": "next build",
    "dev": " next dev",
    "lint": "run-p lint:*",
    "lint:pretty": "pnpm run lib:pretty --list-different",
    "lint:es": "pnpm run lib:es",
    "lint:ts": "pnpm run lib:ts --noEmit",
    "fix:lint": "run-s fix:lint:*",
    "fix:lint:pretty": "pnpm run lib:pretty --write",
    "fix:lint:es": "pnpm run lib:es --fix",
    "test:unit": "pnpm run lib:jest",
    "test:unit:watch": "pnpm run lib:jest --watch --watchAll=true",
    "test:unit:ci": "pnpm run lib:jest --ci --watchAll=false --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci-cli \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "pnpm run build && start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "storybook": "run-s storybook:clean storybook:dev",
    "storybook:build": "build-storybook -s public -o storybook-build",
    "storybook:clean": "rimraf -r node_modules/.cache storybook-build",
    "storybook:dev": "start-storybook -p 6006 -s ./public",
    "boot": "run-s clean build start",
    "clean": "rimraf -rf .next .vercel coverage storybook-build",
    "prepare": "pnpm exec husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "@next/font": "13.0.5",
    "@runts/react": "2.2.0",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tanstack/react-query": "4.16.1",
    "class-variance-authority": "0.3.0",
    "clsx": "1.2.1",
    "dayjs": "1.11.6",
    "debug": "4.3.4",
    "framer-motion": "7.6.12",
    "just-random": "3.1.1",
    "lodash": "4.17.21",
    "next": "13.0.5",
    "next-base64": "1.1.0",
    "next-connect": "0.13.0",
    "nextjs-cors": "2.1.2",
    "nprogress": "0.2.0",
    "prism-theme-vars": "0.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.39.5",
    "react-icons": "4.6.0",
    "react-medium-image-zoom": "5.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@dotenv-cloak/cli": "2.0.0",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/addon-storysource": "6.5.13",
    "@storybook/builder-webpack5": "6.5.13",
    "@storybook/manager-webpack5": "6.5.13",
    "@storybook/react": "6.5.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/debug": "4.1.7",
    "@types/gtag.js": "0.0.12",
    "@types/jest": "29.2.3",
    "@types/lodash": "4.14.190",
    "@types/ms": "0.7.31",
    "@types/node": "18.11.9",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "@yonycalsin/eslint-plugin-import-sort": "5.1.0",
    "@yonycalsin/prettier-config-react-typescript": "1.0.1",
    "autoprefixer": "10.4.13",
    "babel-loader": "9.1.0",
    "cross-env": "7.0.3",
    "cypress": "10.11.0",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "eslint-config-prettier": "8.5.0",
