    "jest-watch-typeahead": "2.2.1",
    "lint-staged": "13.0.3",
    "msw": "0.47.4",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.19",
    "prettier": "2.8.0",
    "react-test-renderer": "18.2.0",
    "rimraf": "3.0.2",
