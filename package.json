{
  "name": "@yonycalsin/yonycalsin.com",
  "version": "6.0.0",
  "private": true,
  "description": "Personal website running on Nextjs, Typescript, React, and Node.js.",
  "keywords": [],
  "author": "Yony Calsin",
  "main": "n/a",
  "scripts": {
    "boot": "yarn clean  && yarn build && yarn start",
    "build": "next build",
    "check:format": "yarn prettier -- --list-different",
    "clean": "rimraf -rf .next .contentlayer .vercel",
    "contentlayer:build": "./node_modules/.bin/contentlayer build",
    "dev": " next dev",
    "fetch:submodules": "yarn script:run -- scripts/fetch-private-submodules/index.ts",
    "format": "yarn prettier -- --write",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint -- --fix",
    "prettier": "prettier \"**/*.{js,json,ts,tsx,scss,css,md}\"",
    "script:run": "ts-node --project scripts/tsconfig.json",
    "start": "next start",
    "storybook": "yarn storybook:clean && yarn storybook:dev",
    "storybook:build": "yarn contentlayer:build && npx build-storybook -s public -o storybook-build",
    "storybook:clean": "rimraf -r node_modules/.cache storybook-build",
    "storybook:dev": "start-storybook -p 6006 -s ./public",
    "test:unit": "jest --watch",
    "test:unit:ci": "jest --ci",
    "typecheck": "tsc --noEmit",
    "vercel-build": "prisma generate && yarn fetch:submodules && next build",
    "test:static": "yarn check:format && yarn lint && yarn typecheck",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  },
