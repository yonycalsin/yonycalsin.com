{
  "name": "personal-website",
  "version": "2.0.1",
  "description": "Personal website running on Nextjs, Typescript, React, and Node.js.",
  "keywords": [],
  "license": "MIT",
  "author": "Yony Calsin",
  "main": "n/a",
  "scripts": {
    "boot": "npm run clean  && npm run build && npm run start",
    "build": "next build",
    "check:format": "npm run prettier -- --list-different",
    "clean": "rimraf -rf .next node_modules/.contentlayer .vercel",
    "contentlayer:build": "./node_modules/.bin/contentlayer build",
    "dev": " next dev",
    "format": "npm run prettier -- --write",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.{js,json,ts,tsx,scss,css,md}\"",
    "start": "next start",
    "storybook": "npm run storybook:clean && npm run storybook:dev",
    "storybook:build": "npm run contentlayer:build && npx build-storybook -s public -o storybook-build",
    "storybook:clean": "rimraf -r node_modules/.cache storybook-build",
    "storybook:dev": "start-storybook -p 6006 -s ./public",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "theme-preview:build": "tailwind-config-viewer export ./theme-preview-build",
    "typecheck": "tsc --noEmit",
    "vercel-build": "prisma generate && TZ=utc sh ./scripts/vercel-submodule-workaround.sh && next build"
  },
  "dependencies": {
    "@chaitin/querystring": "^1.1.0",
    "@notionhq/client": "^0.4.11",
    "@prisma/client": "^3.5.0",
