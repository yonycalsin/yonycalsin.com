    "@storybook/addon-storysource": "7.4.4",
    "@storybook/builder-webpack5": "7.4.4",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "7.4.4",
    "@testing-library/jest-dom": "6.1.3",
