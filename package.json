{
  "name": "@yonycalsin/yonycalsin.com",
  "version": "0.0.0",
  "private": true,
  "description": "Personal website running on Nextjs, Typescript, React, and Node.js.",
  "author": "Yony Calsin",
  "main": "n/a",
  "scripts": {
    "lib:ts": "tsc",
    "lib:es": "eslint --max-warnings=0 --ext .js,.jsx,.ts,.tsx .",
    "lib:pretty": "prettier \"**/*.{js,json,ts,tsx,scss,css,md}\"",
    "lib:jest": "cross-env TZ=\"America/Los_Angeles\" jest --env=jsdom --colors",
    "start": "next start",
    "build": "next build",
    "dev": " next dev",
    "lint": "run-p lint:*",
    "lint:pretty": "pnpm run lib:pretty --list-different",
    "lint:es": "pnpm run lib:es",
    "lint:ts": "pnpm run lib:ts --noEmit",
    "fix:lint": "run-s fix:lint:*",
    "fix:lint:pretty": "pnpm run lib:pretty --write",
    "fix:lint:es": "pnpm run lib:es --fix",
    "test:unit": "pnpm run lib:jest",
    "test:unit:watch": "pnpm run lib:jest --watch --watchAll=true",
    "test:unit:ci": "pnpm run lib:jest --ci --watchAll=false --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci-cli \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "pnpm run build && start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "storybook": "run-s storybook:clean storybook:dev",
    "storybook:build": "build-storybook -s public -o storybook-build",
    "storybook:clean": "rimraf -r node_modules/.cache storybook-build",
    "storybook:dev": "start-storybook -p 6006 -s ./public",
    "boot": "run-s clean build start",
    "clean": "rimraf -rf .next .vercel coverage storybook-build",
    "prepare": "pnpm exec husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@radix-ui/react-avatar": "1.0.3",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-separator": "1.0.3",
    "@radix-ui/react-slot": "1.0.2",
    "@rqbazan/set-initial-color-mode": "1.2.0",
    "@runts/react": "2.2.0",
