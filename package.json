    "framer-motion": "10.16.4",
    "just-random": "3.2.0",
    "lodash": "4.17.21",
    "next": "13.5.2",
    "next-base64": "1.1.0",
    "nextjs-cors": "2.1.2",
    "nprogress": "0.2.0",
    "prism-theme-vars": "0.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.46.2",
    "react-icons": "4.11.0",
    "react-medium-image-zoom": "5.1.8",
    "tailwind-merge": "1.14.0",
    "tailwindcss-animate": "1.0.7"
