    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "@yonycalsin/eslint-plugin-import-sort": "1.0.7",
    "@yonycalsin/prettier-config-react-typescript": "1.0.1",
    "babel-loader": "8.2.5",
